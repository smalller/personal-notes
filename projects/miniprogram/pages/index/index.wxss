/* 抽奖 2 */
.award_viae{
  position: relative;
  height: 190rpx;
  margin: 0 20rpx;
}
.award_bg{
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  width: 100%;
}
.award_hand{
  position: absolute;
  left: -20rpx;
  top: 44rpx;
  z-index: 2;
  width: 160rpx;
  animation: hand_ani 2.5s infinite linear;
}
@keyframes hand_ani{
  0% {
    transform:translate(0,0);
  }
  50%{
    transform:translate(15rpx,-15rpx);
  }
  100% {
    transform:translate(0,0);
  }
} 
.award_btn{
  position: absolute;
  right: 32rpx;
  top: 60rpx;
  z-index: 2;
  width: 100rpx;
  animation: factor .8s infinite linear;
}
@keyframes factor{
  0% {
    transform:scale(1);
  }
  50%{
    transform:scale(1.08);
  }
  100% {
      transform: scale(1);
  }
} 

