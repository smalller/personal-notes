.animate_change{
  animation: shake 1s linear;
}

@keyframes shake{
  20% {
      transform: rotate(30deg);
  }
  40% {
      transform: rotate(-30deg);
  }
  60% {
      transform: rotate(15deg);
  }
  80% {
      transform: rotate(-15deg);
  }
  100% {
      transform: rotate(0deg);
  }
} 

