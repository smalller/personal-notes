page{
  background: #161823;
}
.homePage {
  height: 100%;
  font-family: PingFangSC-Regular, PingFang SC;
}
.homePage .top-bg {
  /* width: 100%; */
  padding: 0 24rpx;
}
.homePage .top-bg image {
  width: 100%;
  height: 362rpx;
  border-radius: 10rpx;
}
.homePage .outlet-circle {
  position: relative;
}
.homePage .outlet-circle image {
  width: 168rpx;
  height: 168rpx;
  position: absolute;
  left: calc(50% - 168rpx/2);
  top: -84rpx;
  border-radius: 50%;
  border: 8rpx solid white;
  box-sizing: border-box;
}
.homePage .middle-text {
  text-align: center;
  margin-top: 132rpx;
  margin-bottom: 72rpx;
}

.homePage .middle-text view {
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
}
.homePage .middle-text .one {
  font-size: 30rpx;
  font-weight: 600;
  color: #FFFFFF;
}
.homePage .middle-text .two {
  margin-top: 16rpx;
  margin-bottom: 16rpx;
  color: #C2C5D9;
  font-size: 26rpx;
}
.homePage .middle-text .three {
  color: #C2C5D9;
  font-size: 26rpx;
  margin-bottom: 52rpx;
}
.max_price{
  width: 546rpx;
  height: 176rpx;
  background: #262833;
  border-radius: 4rpx;
  margin: 0 auto;
  text-align: center;
}
.homePage .middle-text .four{
  color: #E4E4E4;
}
.homePage .middle-text .top_price{
  color: #EF5954;    
  font-size: 48rpx;
  border-radius: 8rpx;
  padding-top: 40rpx;
  font-weight: 600;
  margin-bottom: 20rpx;
}
.top_price text{
  font-size: 24rpx;
}
.homePage .botm-box {
  height: 520rpx;
  margin:0 20rpx ;
  display: flex;
  display: flex;
  justify-content: space-between;
  border-top:2rpx solid #23263C ;
}
.homePage .botm-box > view:first-child {
  border-right: 2rpx solid #23263C;
}
.homePage .botm-box > view:last-child {
  box-sizing: border-box;
}
.homePage .botm-box > view {
  margin-top: 100rpx;
  width: 50%;
  height: 300rpx;
  font-size: 26rpx;
  color: #E4E4E4;
}
.homePage .botm-box > view .top-txt {
  display: flex;
  margin-top: 22rpx;
  text-align: center;
  justify-content: center;
}
/* .homePage .botm-box > view .top-txt text {
  margin-right: 12rpx;
} */
.homePage .botm-box > view .top-txt image {
  width: 24rpx;
  height: 24rpx;
}
.homePage .botm-box > view .middle-txt {
  font-size: 26rpx;
  text-align: center;
  color: #8B8EA5;
}
.homePage .botm-box > view .middle-txt text {
  color: #EF5954;
  margin-right: 10rpx;
  font-size: 40rpx;
}
.homePage .botm-box > view .btn-bg {
  margin: 0 auto;
  width: 244rpx;
  height: 84rpx;
  margin-top: 42rpx;
}
.homePage .botm-box > view .btn-bg image {
  width: 100%;
  height: 100%;
}
.homePage .mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.15);
  z-index: 2;
}
.homePage .modalDlg {
  font-size: 28rpx;
  font-weight: 400;
  color: #000;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  line-height: 40rpx;
  margin-left: -256rpx;
  width: 512rpx;
  height: 360rpx;
  position: fixed;
  top: 430rpx;
  left: 50%;
  box-sizing: border-box;
  padding: 70rpx 48rpx;
  border-radius: 10rpx;
  background-color: #fff;
  z-index: 3;
}
.homePage .modalDlg text {
  font-size: 36rpx;
  font-weight: 600;
  color: #FFC100;
}
.login_btn{
  width: 244rpx;
  height: 84rpx;
  padding: 0;
  border: 0;
  margin-top: 42rpx;
}
.login_btn ::after{
  border: none;
}
.login_btn[plain] {
  border:none;
}
.login_btn image{
  width: 100%;
  height: 100%;
}