page{
    background: #161823; 
}
.personSet {
  width: 350rpx;
  margin: 240rpx auto 0;
}

.personSet .top {
  font-size: 32rpx;
  color: #FFFFFF;
  line-height: 44rpx;
  text-align: center;
  margin-bottom: 24rpx;
}

.personSet .bottom {
  font-size: 36rpx;
  /* font-weight: 600; */
  color: #000000;
  line-height: 50rpx;
  background: #FACE15;
  width: 350rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  border-radius: 10rpx;
}

.keyboard {
  text-align: center;
  width: 100%;
  box-shadow: 0 -3rpx 5rpx 1rpx #dcdcdc;
  border-radius: 14rpx 14rpx 0 0;
  border-top: 1px solid transparent;
  margin-bottom: 168rpx;
}

.keyboard .inp {
  margin: 100rpx auto 24rpx;
  width: 672rpx;
  border-top: 1px solid #979797;
  border-left: 1px solid #979797;
  height: 110rpx;
  display: flex;
  align-items: center;
}

.keyboard .inp text {
  width: 110rpx;
  height: 110rpx;
  border-bottom: 1px solid #FFFFFF;
  border-right: 1px solid #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 140rpx;
  color: #FFFFFF;
  /* line-height: 199rpx; */
}

.keyboard .zh {
  font-size: 24rpx;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 34rpx;
}

.num_board {
  height: 432rpx;
  width: 100%;
  background-color: #D2D5DB;
  position: fixed;
  bottom: 0;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-items: center;
  position: relative;
}
.num_board .iconfont{
  font-size: 66rpx;
  color: #888;
  position: absolute;
  right: 104rpx;
  bottom: 20rpx;
}
.num_board .num_eng {
  width: 234rpx;
  height: 92rpx;
  background-color: #fff;
  box-shadow: 0px 1px 0px 0px #848688;
  border-radius: 10rpx;
  text-align: center;
  font-size: 50rpx;
  font-family: Helvetica;
  color: #000000;
  line-height: 60rpx;
  line-height: 92rpx;
}
.num_board .bg{
  background-color: #f5f5f5;
}
.key_box{
  position: fixed;
  bottom: 0;
  background: #23263C;
}