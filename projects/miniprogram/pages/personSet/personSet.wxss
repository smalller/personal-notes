  left: 0;
  background: rgba(0, 0, 0, 0.15)
}

.popup {
  width: 654rpx;
  height: 630rpx;
  background-color: white;
  border-radius: 8rpx;
  margin: 0 auto;
  padding: 48rpx 0 0 48rpx;
  position: fixed;
  top: 54rpx;
  left: 46rpx;
  z-index: 2;
  box-sizing: border-box;
}

.popup view {
  font-size: 32rpx;
