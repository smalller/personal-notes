.info_set {
  padding-top: 12rpx;
}

.info_set .info_body {
  /* display: flex; */
  padding: 0 30rpx;
  line-height: 96rpx;
  background: #161823;
}
.bdtop{
  border-bottom: 1px solid #1C1E2C;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}

.info_set .info_body image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 28rpx;
}
.indiv{
  width: 28rpx!important;
  height: 36rpx!important;
  margin-right: 34rpx!important;
  padding-left: 6rpx;
}
.info_body input{
  font-size: 28rpx;
}
.nl{
  flex: .4;
}
.nm{
  font-size: 28rpx;
  color: #CCCCCC;
  flex: .46;
}
.nr{
  flex: .24;
  text-align: right;
  font-size: 28rpx;
}
.btn_tel{
  background-color:#161823;
  padding: 0;
  margin: 0;
  color: #C9C9D3;
  line-height: 88rpx;
}
.btn_tel .l-ic{
  margin-left: 32rpx;
}
.btn_tel::after{
  border: none;
}
.l-ic{
  font-size: 28rpx;
  color: #C9C9D3;
  margin-left: 24rpx;
}
.info_set .info_body .putong {
  font-size: 28rpx;
