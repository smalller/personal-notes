.self_bg{
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, .5);
    width: 100%;
    height: 100%;
    z-index: 250;
}
.self_dio_bg {
    position: fixed;
    top: calc(50% - 120rpx);
    left: calc(50% - 260rpx);
    background: #232323;
    width: 500rpx;
    height: 240rpx;
    border-radius: 12rpx;
    z-index: 251;
}
.self_dio_bg .self_box {
    padding-top: 54rpx;
}
.self_dio_bg .self_title {
    font-size: 32rpx;
    margin-bottom: 45rpx;
    color: #CECECE;
    text-align: center;
    font-weight: 600;
}
.self_dio_bg .self_tips {
    margin-bottom: 52rpx;
    text-align: center;
    color: #FFFFFF;
    font-size: 28rpx;
    padding: 0 20rpx;
}
.self_dio_bg .self_btm_btn {
    display: flex;
    align-items: center;
    justify-content: center;
    border-top: 1px solid #2C3043;
}
.self_dio_bg .self_btm_btn .btn {
    border-radius: 0;
    flex: 1;
    font-size: 28rpx;
    font-weight: 600;
    background: #232323;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
}
.self_dio_bg .self_btm_btn .btn:nth-child(1) {
    color: #9D9FA8;
    border-right: 1px solid #2C3043;
    box-sizing: border-box;
    border-radius: 0 0 0 12rpx;
}
.self_dio_bg .self_btm_btn .btn:nth-child(2) {
    color: #FFFFFF;
    border-radius: 0 0 12rpx 0;
}